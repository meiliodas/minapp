.head{
  width: 750rpx;
  height: 100rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.tra{
  width: 116px;
  height: 32px;
  padding: 0 20rpx;
}

.search{
  width: 24px;
  height: 24px;
  padding: 0 20rpx;
}

.nav {
  width: 750rpx;
  height: 300rpx;
}

.icon {
  width: 100rpx;
  height: 100rpx;
}
.icon-list {
  height: 240rpx; 
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 40rpx;
}
.icon-list view {
  display: flex;
  flex-direction: column;
}
/* 列表 */
.list {
  margin-top: 20rpx;
  background: #fff;
}
.list-nav {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
}
.list-item{
  display: flex;
  margin-top: 10rpx;
}
.list-item-left {
  width: 420rpx;
  height: 186rpx;
}
.list-item-left image{
  width: 100%;
  height: 100%;
}

.list-item-right {
  font-size: 12px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.list-head {
  font-size: 16px;
}

.list-zw {
  color: #ccc;
}

.list-mony {
  color: #f00;
}

.list-item-right view:last-child {
  display: flex;
  justify-content: space-between;
}

.tj{
  display: flex;
  flex-wrap: wrap;
  justify-content:space-around;
}

.tj-item{
  display: flex;
  margin-top: 10rpx;
  
}

.tj-item-top{
  width: 330rpx;
  height: 186rpx;
  /* flex-wrap: wrap; */
}

.tj-item-top image{
  width: 100%;
  height: 100%;
}

.tj-item-last {
  font-size: 12px;
  display: flex;
  flex-direction: column;
  /* justify-content: space-between; */
}

.tj-last {
  width: 330rpx;
  display: flex;
  justify-content: space-between;
}

.tj-head{
  font-size: 16px;
}

.tj-mony{
  color: #f00;
}